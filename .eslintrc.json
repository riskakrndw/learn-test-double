{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-console": "warn",
      "eqeqeq": "error"
    },
    "plugins": ["jest"]
  }